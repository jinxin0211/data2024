var readline=require("readline-sync")

//input weight
var weight=readline.questionFloat("please input ur weight(KG)")
var height=readline.questionFloat("please input ur height(CM)")
var bmi=weight/((height/100)**2);

console.log("hello!your bmi value is"+bmi);


